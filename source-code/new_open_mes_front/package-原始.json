  "scripts": {
    "i": "pnpm install",
    "dev": "vite --mode dev",
    "front": "vite --mode front",
    "ts:check": "vue-tsc --noEmit",
    "build:base": "node --max_old_space_size=8000 ./node_modules/vite/bin/vite.js build --mode base",
    "build:prod": "node --max_old_space_size=8000 ./node_modules/vite/bin/vite.js build --mode prod",
    "build:dev": "node --max_old_space_size=8000 ./node_modules/vite/bin/vite.js build --mode dev",
